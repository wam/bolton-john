{
  "name": "Bolton John",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Enhance your news media experience",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
